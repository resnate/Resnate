<div class="mobileDivider">
			<%= image_tag "https://graph.facebook.com/#{current_user.uid}/picture?width=100&height=100", class: "mobileProfilePic" %>
			<div class="mobileInfo">
				<div><%= current_user.name %></div>
				<div><%= current_user.level_name %></div>
				<div><%= current_user.points %></div>
			</div>
		</div>
		<div class="mobileDivider">
			Write a Review
		</div>
		<div class="mobileDivider">
			<% if @reviews.count == 0 %>
			Your Reviews
			<% else %>
				<% if @review.reviewable_type == "PastGig" %>
					<img src="https://api.songkick.com/api/3.0/events/<%= PastGig.find(@review.reviewable_id).songkick_id %>.json?apikey=Pxms4Lvfx5rcDIuR&jsoncallback=?" class="mobileReviewImg">
				<% else %>
					<img src="https://img.youtube.com/vi/<%= Song.find(@review.reviewable_id).content %>/hqdefault.jpg" class="mobileReviewImg">
				<% end %>
				<div class="mobileInfo">
					Your Reviews
				</div>
			<% end %>
		</div>
		<div class="mobileDivider">
			<% if @user.playlists.count === 0 %>
				Your Playlists
			<% else %>
				<% @user.playlists.limit(2).each do |playlist| %>
      				<% unless playlist.content.nil? %>
      					<% img1 = "a" %>
            			<% JSON.parse(playlist.content)[0].each do |k, v| %>
              				<% img1 = v %>
            			<% end %>
            			<img src="https://img.youtube.com/vi/<%= img1 %>/hqdefault.jpg">
      				<% end %>
      			<% end %>
			<% end %>
		</div>