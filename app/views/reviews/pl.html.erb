<head>
	<meta property="og:title" content="<%= User.find(@review.user_id).first_name %>'s Review of <%= @name %>" />
    <meta property="og:description" content="Resnate: The Music Social Network." />
    <% if @review.reviewable_type == "Song" %>
    	<meta property="og:image" content="https://img.youtube.com/vi/<%= Song.find(@review.reviewable_id).content %>/hqdefault.jpg" />
    <% else %>
    <meta property="og:image" content="https://graph.facebook.com/<%= User.find(@review.user_id).uid %>/picture?width=200&height=200" />
    <% end %>
 </head>
