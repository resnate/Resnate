<html xmlns:fb="http://ogp.me/ns/fb#">
	<head>
	<%= stylesheet_link_tag "application", media: "screen",
                                           "data-turbolinks-track" => true %>
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
    <%= csrf_meta_tags %>
	<meta property="og:title" content="<%= @name %>" />
    <meta property="og:description" content="Resnate: The Music Social Network." />
    <meta property="og:image" content="<%= @image %>" />
    </head>

    <body>
    	<div class="hiddenReviewID"><%= params[:id] %></div>
    	<div id="publicReview"></div>
    	</div>
    </body>
 

	<script type="text/javascript">

 	$(function(){
 		$.get('/reviews/' + $('.hiddenReviewID').html(), function(result){
 			$('#publicReview').html(result);
 			$('#publicReview #reviewBody').append("<h1 id='publicSignUp'>Sign up to <a href=\"/\">resnate.com</a> to read and write reviews for your favourite artist's albums and gigs.</h1>")
 		});
 	});

 	</script>
</html>